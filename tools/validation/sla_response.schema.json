{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SLA Subgraph Response (data object)",
  "type": "object",
  "properties": {
    "serviceHealthMetrics": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "totalProofs": { "type": "string" },
          "downtimeCount": { "type": "string" },
          "latencyCount": { "type": "string" },
          "blockLatencyAvg": { "type": "string" },
          "timeLatencyAvg": { "type": "string" },
          "consensusExecutions": { "type": "string" },
          "violations": { "type": "string" }
        },
        "required": [
          "id",
          "totalProofs",
          "downtimeCount",
          "latencyCount",
          "blockLatencyAvg",
          "timeLatencyAvg",
          "consensusExecutions",
          "violations"
        ]
      }
    }
  },
  "required": ["serviceHealthMetrics"]
}
